<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.MeiriyinshiDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.MeiriyinshiEntity" id="meiriyinshiMap">
        <result property="riqi" column="riqi"/>
        <result property="zaocan" column="zaocan"/>
        <result property="wancan" column="wancan"/>
        <result property="zhongcan" column="zhongcan"/>
        <result property="beizhu" column="beizhu"/>
        <result property="yonghuzhanghao" column="yonghuzhanghao"/>
        <result property="userid" column="userid"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.MeiriyinshiVO" >
		SELECT * FROM meiriyinshi  meiriyinshi         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.MeiriyinshiVO" >
		SELECT  meiriyinshi.* FROM meiriyinshi  meiriyinshi 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.MeiriyinshiView" >

		SELECT  meiriyinshi.* FROM meiriyinshi  meiriyinshi 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.MeiriyinshiView" >
		SELECT * FROM meiriyinshi  meiriyinshi <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
